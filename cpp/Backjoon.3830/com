
            if(com == '!'){
                    >> a >> b >> w;
                merge(a, b, w);
            }else{
                cin >> a >> b;
                auto x = find(a,0);
                auto y = find(b,0);
                if(x.first != y.first){
                    printf("UNKNOWN\n");
                }else{
                    cout << -x.second+y.second << "\n";
                }
            }
        }
    }
    return 0;
}[A[A
